Category
    name                        Text
    order                       Int
    UniqueCategoryName          name

SubCategory
    category                    CategoryId
    name                        Text
    UniqueSubCategoryName       category name

SubCategoryProduct
    subCategory                 SubCategoryId
    product                     ProductId
    top                         Bool
    UniqueSubCategoryProduct    subCategory product

BirthList
    name                        Text
    UniqueBirthListName         name

BirthListProduct
    birthList                   BirthListId
    product                     ProductId
    reserved                    Bool
    UniqueBirthListProduct      birthList product

Product
    name                        Text
    ref                         Text Maybe
    shortDesc                   Text
    desc                        Textarea
    details                     Textarea
    price                       Double Maybe
    available                   Bool

Picture
    product                     ProductId
    extension                   Text
